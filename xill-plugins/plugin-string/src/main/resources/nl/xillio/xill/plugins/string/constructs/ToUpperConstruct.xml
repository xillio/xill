<?xml version="1.0" encoding="utf-8"?>
<function>
  <description>
	Returns the string uppercased.
  </description>
  <examples>
    <example description="Executing:"><![CDATA[
    	String.toUpper("LoWer");
    ]]></example>
    <example description="Will return:"><![CDATA[
		"LOWER"
    	]]>
    </example>
  </examples>
  <references>
    <reference package="String">toLower</reference>
  </references>
  <searchTags>
  		case, upper
  </searchTags>
</function>