<?xml version="1.0" encoding="utf-8"?>
<function>
    <description>
        Returns the absolute value of the provided value.
    </description>
    <examples>
        <example title="Usage">
            <header>Running</header>
            <code>
                System.print(Math.abs(6))
                System.print(Math.abs(-5))
                System.print(Math.abs(-3.14))
            </code>
            <header>Will print</header>
            <code>
                6
                5
                3.14
            </code>
        </example>
    </examples>
</function>