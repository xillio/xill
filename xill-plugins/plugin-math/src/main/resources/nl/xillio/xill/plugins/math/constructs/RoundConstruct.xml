<?xml version="1.0" encoding="utf-8"?>
<function>
    <description>
        Rounds the provided value to the nearest whole number.
    </description>
    <examples>
        <example title="Usage">
            <header>Running</header>
            <code>
                System.print(Math.round(6))
                System.print(Math.round(3.14))
                System.print(Math.round(-3.78))
            </code>
            <header>Will print</header>
            <code>
                6
                3
                -4
            </code>
        </example>
    </examples>
</function>