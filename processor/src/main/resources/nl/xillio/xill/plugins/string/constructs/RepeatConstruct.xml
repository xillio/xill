<?xml version="1.0" encoding="utf-8"?>
<function>
    <description>
Repeats a string.
    </description>
    <examples>
        <example title="Usage">
            <code>
                use System, String;

                System.print(String.repeat("thisIsAString", 3));
                // Result: thisIsAStringthisIsAStringthisIsAString
            </code>
        </example>
    </examples>
</function>