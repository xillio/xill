<?xml version="1.0" encoding="utf-8"?>
<function Tag="datediff">
  <name>datediff</name>
  <parameters>
    <param name="date1,">
      <type>date</type>
    </param>
    <param name="date2,">
      <type>date</type>
    </param>
    <param name="absolute ">
      <type>boolean</type>
    </param>
  </parameters>
  <description><![CDATA[Returns the absolute difference between date 1 and date 2. Optionally absolute can be set to false to return the relative difference.]]></description>
  <examples>
    <example description="Executing:"><![CDATA[log(datediff(date(1382599121764), date()));
]]></example>
    <example description="prints:"><![CDATA[{
  "day": 1,
  "hour": 0,
  "millisecond": 453,
  "minute": 10,
  "second": 49,
  "timestamp": 87049453
}
]]></example>
    <example description="Executing:"><![CDATA[log(datediff(date(1382599121764), date(), false));
]]></example>
    <example description="prints:"><![CDATA[{
  "day": -1,
  "hour": 0,
  "millisecond": -456,
  "minute": -10,
  "second": -49,
  "timestamp": -87049456
}
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag>Date functions</tag>
  </tags>
</function>