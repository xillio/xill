<?xml version="1.0" encoding="utf-8"?>
<function Tag="es_put">
  <name>es_put</name>
  <parameters>
    <param name="index,">
      <type>string</type>
    </param>
    <param name="type,">
      <type>string</type>
    </param>
    <param name="source ,">
      <type>list</type>
    </param>
    <param name="id ,">
      <type>&lt;string,void&gt;</type>
    </param>
    <param name="client ">
      <type>string</type>
    </param>
  </parameters>
  <description><![CDATA[Index a document source in elasticsearch under given index and type with id. Returns true if the document was inserted properly, or false otherwise. If id is null, it will be automatically generated by elasticsearch. If an id is specified, the combination of index, type and id must be unique.]]></description>
  <examples>
    <example description="Add a pdf document to an index:"><![CDATA[path = "C:/TheHistoryOfComputerScience.pdf";
es_connect();
]]></example>
    <example description="//Creating the document object"><![CDATA[document = metadatafromdocument(path);
document.content = textfromdocument(path);
]]></example>
    <example description="put = es_put(&quot;literature&quot;, &quot;computerscience&quot;, document);"><![CDATA[if (!put) {
        //Error
}
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag>Elasticsearch</tag>
  </tags>
</function>