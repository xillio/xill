<?xml version="1.0" encoding="utf-8"?>
<function Tag="lowercase">
  <name>lowercase</name>
  <parameters>
    <param name="text">
      <type>&lt;string,void&gt;</type>
    </param>
  </parameters>
  <description><![CDATA[Returns the lowercase string of text, which must also be a string.]]></description>
  <examples>
    <example description="log (lowercase(&quot;3... 2... 1... START... Genius!&quot;));"><![CDATA[]]></example>
    <example description="will print:"><![CDATA[3... 2... 1... start... genius!
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag> uppercase</tag>
    <tag>Text</tag>
  </tags>
</function>