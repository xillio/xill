<?xml version="1.0" encoding="utf-8"?>
<function Tag="mongo_drop">
  <name>mongo_drop</name>
  <parameters>
    <param name="collection,">
      <type>string</type>
    </param>
    <param name="mongo ">
      <type>string</type>
    </param>
  </parameters>
  <description><![CDATA[Drops the collection from the database identified by mongo. If mongo is not provided the last active connection will be used.]]></description>
  <examples>
    <example description="General usage:"><![CDATA[mongo_connect(settings.db);
mongo_drop(settings.collection);
]]></example>
    <example description="Where the settings might be:"><![CDATA[settings = {
    db = "movieDB",
    collection = "ratings"
};
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag> mongo_delete</tag>
    <tag>MongoDB</tag>
  </tags>
</function>