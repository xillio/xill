<?xml version="1.0" encoding="utf-8"?>
<function Tag="urldecode">
  <name>urldecode</name>
  <parameters>
    <param name="text">
      <type>&lt;string,void&gt;</type>
    </param>
  </parameters>
  <description><![CDATA[Returns the URL-decoded version of string text. Useful for reading URL’s with GET variables in them. Note that both %20 and the plus sign are converted to a space character.]]></description>
  <examples>
    <example description="Executing:"><![CDATA[log (urldecode("Together%2C%20we+spent%20%2450.95"));
]]></example>
    <example description="will print:"><![CDATA[Together, we spent $50.95
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag> urlencode</tag>
    <tag>Encoding / Decoding</tag>
  </tags>
</function>