<?xml version="1.0" encoding="utf-8"?>
<function Tag="deletefile">
  <name>deletefile</name>
  <parameters>
    <param name="uri">
      <type>string</type>
    </param>
  </parameters>
  <description><![CDATA[Deletes the file located at uri. If the file cannot be deleted an error is logged.]]></description>
  <examples>
    <example description="Deletes the file test.txt:"><![CDATA[deletefile ("C:/tmp/note.txt");
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag>File</tag>
  </tags>
</function>