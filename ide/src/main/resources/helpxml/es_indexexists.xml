<?xml version="1.0" encoding="utf-8"?>
<function Tag="es_indexexists">
  <name>es_indexexists</name>
  <parameters>
    <param name="index,">
      <type>string</type>
    </param>
    <param name="client ">
      <type>string</type>
    </param>
  </parameters>
  <description><![CDATA[Returns true if the specified index exists on cluster client, and false otherwise.]]></description>
  <examples>
    <example description="Check if the index “someindex” exists:"><![CDATA[es_connect();
exists = es_indexexists("someindex");
if (exists) {
        //Perform actions
}
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag>Elasticsearch</tag>
  </tags>
</function>