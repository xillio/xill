<?xml version="1.0" encoding="utf-8"?>
<function Tag="dateinfo">
  <name>dateinfo</name>
  <parameters>
    <param name="date">
      <type>date</type>
    </param>
  </parameters>
  <description><![CDATA[Returns a list of individual properties for the given date such has year, day, week, readable month, timestamp, etc.]]></description>
  <examples>
    <example description="Executing:"><![CDATA[log(dateinfo(date()));
]]></example>
    <example description="prints:"><![CDATA[{
  "day": 24,
  "dayname": "Thursday",
  "hour": 9,
  "millisecond": 764,
  "minute": 18,
  "month": 10,
  "monthname": "October",
  "second": 41,
  "timestamp": 1382599121764,
  "week": 43,
  "weekofmonth": 4,
  "year": 2013
}
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag>Date functions</tag>
  </tags>
</function>