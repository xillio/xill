<?xml version="1.0" encoding="utf-8"?>
<function Tag="es_get">
  <name>es_get</name>
  <parameters>
    <param name="index,">
      <type>string</type>
    </param>
    <param name="type,">
      <type>string</type>
    </param>
    <param name="id,">
      <type>string</type>
    </param>
    <param name="client ">
      <type>string</type>
    </param>
  </parameters>
  <description><![CDATA[Returns the document with the given id, index and type from cluster client.]]></description>
  <examples>
    <example description="Executing the following code:"><![CDATA[es_connect();
MonaLisa = es_get("art", "paintings", "monalisa");
log(MonaLisa);
]]></example>
    <example description="might print something like this:"><![CDATA[{
        "_index": "art",
        "_type": "paintings",
        "_id": "monalisa",
        "_source": {
                "title": "Mona Lisa",
                "artist": "Leonardo da Vinci"
        }
}
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag>Elasticsearch</tag>
  </tags>
</function>