<?xml version="1.0" encoding="utf-8"?>
<function Tag="urlencode">
  <name>urlencode</name>
  <parameters>
    <param name="text">
      <type>&lt;string,void&gt;</type>
    </param>
  </parameters>
  <description><![CDATA[Returns the URL-encoded (percent-encoded) version of string text. Essential for creating URL’s with GET variables in them. Note that spaces are converted to plus signs.]]></description>
  <examples>
    <example description="Executing:"><![CDATA[log (urlencode("Together, we spent $50.95"));
]]></example>
    <example description="will print:"><![CDATA[Together%2C+we+spent+%2450.95
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag> urldecode</tag>
    <tag>Encoding / Decoding</tag>
  </tags>
</function>