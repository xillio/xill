<?xml version="1.0" encoding="utf-8"?>
<function Tag="listreverse">
  <name>listreverse</name>
  <parameters>
    <param name="list,">
      <type>&lt;list,void&gt;</type>
    </param>
    <param name="recursive ">
      <type>boolean</type>
    </param>
  </parameters>
  <description><![CDATA[Returns the reversed list. The optional parameter recursive is a boolean. When set to true (or omitted) the list is recursively reversed, when set to false only the outer-most level of the the list is reversed.]]></description>
  <examples>
    <example description="Using the following list for all examples:"><![CDATA[mylist = {
    "0b",
    "b"=1,
    "c"={"3"="b",
        "2"="c"},
    "d"={"2" ="a",
        "1"="b"},
    "e"=2,
    "00"
};
]]></example>
    <example description="Reverse list and all sub lists"><![CDATA[log(listreverse(mylist));
]]></example>
    <example description="will output:"><![CDATA[{
    "00",
    e = 2,
    d = {
        1 = "b",
        2 = "a"
    },
    c = {
        2 = "c",
        3 = "b"
    },
    b = 1,
    "0b"
}
]]></example>
    <example description="Reverse outer list only"><![CDATA[log(listreverse(mylist, false));
]]></example>
    <example description="will output:"><![CDATA[{
    "00",
    e = 2,
    d = {
        2 = "a",
        1 = "b"
    },
    c = {
        3 = "b",
        2 = "c"
    },
    b = 1,
    "0b"
}
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag>List</tag>
  </tags>
</function>