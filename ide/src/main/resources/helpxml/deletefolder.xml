<?xml version="1.0" encoding="utf-8"?>
<function Tag="deletefolder">
  <name>deletefolder</name>
  <parameters>
    <param name="uri">
      <type>string</type>
    </param>
  </parameters>
  <description><![CDATA[Deletes the folder (and its contents) located at uri. If the folder cannot be deleted an error is logged.]]></description>
  <examples>
    <example description="Deletes the folder C:/tmp/123:"><![CDATA[deletefolder ("C:/tmp/123");
]]></example>
  </examples>
  <Applications />
  <tags>
    <tag>File</tag>
  </tags>
</function>